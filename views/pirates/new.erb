<h1>Make your form here</h1>
<form method = "post" action = "/pirates">
<h2> Enter Pirate Information Below</h2>
<label for="pirate_name">Pirate Name: </label> 
<input type="text" name=pirate[name] id="pirate_name"> 
<label for="pirate_w">Pirate Weight: </label>
<input type = "integer" name = pirate[weight] id= "pirate_w">
<label for="pirate_h">Pirate Height: </label>
<input type = "integer" name = pirate[height] id= "pirate_h">
<br>

<h2> Enter Ship Information Below</h2>
<label for="ship_name_1">Ship Name: </label>
<input type = "text" name= pirate[ships][][name]  id= "ship_name_1">
<label for="ship_type_1">Ship Type: </label>
<input type = "integer" name = pirate[ships][][type] id= "ship_type_1">
<label for="ship_booty_1">Ship Booty: </label>
<input type = "text" name = pirate[ships][][booty] id= "ship_booty_1">
<label for="ship_name_2">Ship Name: </label>
<input type = "text" name= pirate[ships][][name]  id= "ship_name_2">
<label for="ship_type_2">Ship Type: </label>
<input type = "integer" name = pirate[ships][][type] id= "ship_type_2">
<label for="ship_booty_2">Ship Booty: </label>
<input type = "text" name = pirate[ships][][booty] id= "ship_booty_2">
<button type="submit" value="submit">Submit</button>

</form>